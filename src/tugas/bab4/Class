package BukuAlamat;

public class BukuAlamat {

    private String nama;
    private String alamat;
    private String telepon;
    private String email;

    public BukuAlamat() {
    }

    public BukuAlamat(String a, String b, String c, String d) {
        nama = a;
        alamat = b;
        telepon = c;
        email = d;
    }
